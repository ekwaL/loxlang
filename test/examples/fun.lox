fun fib(n) {
    print "fib " + n;
    if (n <= 1) {
        print "n = " + n + " <=1";
        return n;
    }
    print "n = " + n + " >1";
    var m2 = fib(n - 2);
    var m1 = fib(n - 1);
    print "m2 : " + m2;
    print "m1 : " + m1;
    return m2 + m1;
}

